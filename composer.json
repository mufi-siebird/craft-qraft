{
    "name": "orbital-flight/craft-qraft",
    "description": "An advanced QR Code generator for CraftCMS",
    "type": "craft-plugin",
    "license": "proprietary",
    "version": "1.0.0",
    "support": {
        "email": "flightorbital@gmail.com",
        "issues": "https://github.com/orbital-flight/craft-qraft.git/issues?state=open",
        "source": "https://github.com/orbital-flight/craft-qraft.git",
        "docs": "https://github.com/orbital-flight/craft-qraft/wiki",
        "rss": "https://github.com/orbital-flight/craft-qraft.git/releases.atom"
    },
    "authors": [
        {
          "name": "Orbital Flight"
        }
    ],
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^4.4.0",
        "endroid/qr-code": "^4.8"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "orbitalflight\\qraft\\": "src/"
        }
    },
    "extra": {
        "handle": "qraft",
        "name": "QRaft",
        "developer": "Orbital Flight",
        "documentationUrl": "https://github.com/orbital-flight/craft-qraft/wiki",
        "class": "orbitalflight\\qraft\\Qraft"
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

